    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.17.0",
    "typescript": "^5.7.3"
  }
}